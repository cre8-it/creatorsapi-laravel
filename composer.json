{
    "name": "creatorsapi/creatorsapi-laravel",
    "description": "Laravel wrapper for amazon creatorsapi-php-sdk",
    "type": "library",
    "authors": [
        {
            "name": "Henrik Schumacher",
            "email": "henrik.schumacher@cre8-it.de"
        }
    ],
    "require": {
        "php": "^8.1",
        "cre8-it/creatorsapi-php-sdk": "dev-main",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "CreatorsApi\\Laravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CreatorsApi\\Laravel\\CreatorsApiServiceProvider"
            ],
            "aliases": {
                "CreatorsApi": "CreatorsApi\\Laravel\\Facades\\CreatorsApi"
            }
        }
    }
}
